package com;

public class Demo2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int arr1[][]= {{2,4,6},{3,2,6}};
		int arr2[][]= {{3,4,5},{1,3,4}};
		
		int c[][]=new int[2][3];
		
		for(int i=0;i<2;i++)
		{
			for(int j=0;j<3;j++)
			{
			c[i][j]=arr1[i][j]+arr2[i][j];	
			
			System.out.print(c[i][j]+" ");
			}
			System.out.println();
		}
	}
}
